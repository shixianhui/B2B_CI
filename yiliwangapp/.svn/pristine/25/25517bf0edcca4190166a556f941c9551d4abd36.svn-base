<script type="text/javascript">
	mui.init({
	  gestureConfig:{
	   longtap: true
	  }
	});
	
	mui(".m2-nav").on('tap','li',function(){
		var index = $(this).index();
//		console.log(index)
		if($(this).index()==1){
		$(this).addClass("active").siblings().removeClass("active");
			$('#pintuan').attr('style','display:block;');
			$('#yike').attr('style','display:none;');
			get_my_group_purchase_list();
		}else if($(this).index()==0){
			$(this).addClass("active").siblings().removeClass("active");
			$('#pintuan').attr('style','display:none;');
			$('#yike').attr('style','display:block;');
			get_my_notes_list();
		}else{
			go_to_view('my_release_list.html',{user_id:localStorage.getItem('global_id')})
		}
	});
	
	
	
	
	/*左菜单*/
	document.getElementById('nav_btn').addEventListener('tap',function(){
		$('.mui-off-canvas-backdrop').addClass('mui-active');
		$('.mui-off-canvas-left').addClass('mui-active');
	})
	document.querySelector('.mui-off-canvas-backdrop').addEventListener('tap',function(){
		$('.mui-off-canvas-backdrop').removeClass('mui-active');
		$('.mui-off-canvas-left').removeClass('mui-active');
	})
	/*左菜单结束*/

	mui('.mui-content').on('tap', 'a', function() {
		var id = this.getAttribute('href');
		if (id && ~id.indexOf('.html')) {
			if (id=='my_release_list.html') {
				go_to_view('my_release_list.html',{user_id:localStorage.getItem('global_id')})
			} else{
				go_to_active(id);
			}
		}
	});
	
	window.addEventListener('init_data', function(e) {
		init_data();
	});

	window.addEventListener('changeHeadImage', function(mes) {
		init_data();
	});

	window.onload = function() {
		init_data();
	}

	window.addEventListener('page_init', function(e) {
		init_data();
	});
	
	function init_data() {
		if (localStorage.getItem('global_sid') && localStorage.getItem('global_sid') != 'undefined') {
			$('#user_login').show();
			$('#user_not_login').hide();
			get_user_info();
			get_my_group_purchase_list();
			get_my_notes_list();
		
			clearTimeout(stars);
			get_message_count();
		} else {
			$('#user_not_login').show();
			$('#user_login').hide();
		}

	}


	function get_user_info() {
		if (!is_network(false)) {return;}

		var url = base_url + 'napi/get_user_info?sid=' + localStorage.getItem('global_sid');
		mui.ajax(url, {
			data: {},
			dataType: "json",
		 	type: "get",
			timeout: 10000,
			success: function(res) {
				console.log(JSON.stringify(res))
				if (res.success) {
					store_id = res.data.store_id;
					store_display=res.data.store_display;
			 		localStorage.setItem("global_id", res.data.id);
					localStorage.setItem("global_username", res.data.username);
					localStorage.setItem("global_nickname", res.data.nickname);
					localStorage.setItem("global_real_name", res.data.real_name);
					localStorage.setItem("global_path", res.data.path);
					localStorage.setItem("global_path_thumb", res.data.path_thumb);
					localStorage.setItem("global_sex", res.data.sex);
					localStorage.setItem("global_total", res.data.total);
					localStorage.setItem("global_score", res.data.score);
					localStorage.setItem("global_mobile", res.data.mobile);
					localStorage.setItem("global_is_id_card_auth", res.data.is_id_card_auth);
					localStorage.setItem("global_txt_address", res.data.txt_address);
					localStorage.setItem("global_follow_count", res.data.follow_count);
					localStorage.setItem("global_fans_count", res.data.fans_count);
					localStorage.setItem("global_group_purchase_count", res.data.group_purchase_count);
					localStorage.setItem("global_be_praised_num", res.data.be_praised_num);
					localStorage.setItem("global_slogan", res.data.slogan);
			       	if (localStorage.getItem("global_txt_address") && localStorage.getItem("global_txt_address") != 'undefined') {
			        	$('#user_address').html(localStorage.getItem('global_txt_address'));
			       	}else{
			    	     $('#user_address').html('地址');
			        }
			       	if (localStorage.getItem("global_path_thumb") && localStorage.getItem("global_path_thumb") != 'undefined') {
			             $('#user_image_path').attr('src', localStorage.getItem('global_path_thumb'));
			        }else{
			    	     $('#user_image_path').attr('src', " img/tx.jpg");
			        }
			        
			        if (localStorage.getItem("global_sex")== '0') {
			            $('#sex').removeClass('icon-zuihoudinggao');
			            $('#sex').removeClass('icon-nan');
			            $('#sex').html('保密');
			        }else if (localStorage.getItem("global_sex")== '1'){
			    	    $('#sex').removeClass('icon-zuihoudinggao');
			            $('#sex').addClass('icon-nan');
			            $('#sex').html('');
			        }else{
			        	$('#sex').addClass('icon-zuihoudinggao');
			            $('#sex').removeClass('icon-nan');
			            $('#sex').html('');
			        }
			        
					if (localStorage.getItem('global_sid') && localStorage.getItem('global_sid') != 'undefined') {
						$('#user_login').show();
						$('#user_not_login').hide();
						$('#user_login2').show();
					} else {
						$('#user_not_login').show();
						$('#user_login').hide();
						$('#user_login2').hide();
					}
			        $('#member_score').html(localStorage.getItem('global_score'));
			        $('#member_balance').html(localStorage.getItem('global_total'));
			        $('#follow_count').html(localStorage.getItem('global_follow_count'));
			        $('#fans_count').html(localStorage.getItem('global_fans_count'));
			        $('#group_purchase_count').html(localStorage.getItem('global_group_purchase_count'));
			        $('#be_praised_num').html(localStorage.getItem('global_be_praised_num'));
			        $('#slogan').html(localStorage.getItem('global_slogan'));
			      
			 	} else {
			 		localStorage.removeItem("global_id");
					localStorage.removeItem("global_username");
					localStorage.removeItem("global_nickname");
					localStorage.removeItem("global_real_name");
					localStorage.removeItem("global_path");
					localStorage.removeItem("global_path_thumb");
					localStorage.removeItem("global_sex");
					localStorage.removeItem("global_total");
					localStorage.removeItem("global_score");
					localStorage.removeItem("global_mobile");
					localStorage.removeItem("global_txt_address");
					localStorage.removeItem("global_follow_count");
					localStorage.removeItem("global_fans_count");
					localStorage.removeItem("global_group_purchase_count");
					localStorage.removeItem("global_be_praised_num");
					localStorage.removeItem("global_slogan");
					mui.toast("请登录");
					go_to_active('login.html');
			 	}
			 }, error: error
		});
	}


	
	
	
	
	// 拼团
	function get_my_group_purchase_list() {
		if (!is_network(false)) {return;}
		var url = base_url + 'napi/get_my_group_purchase_list/0/0/20/1?sid=' + localStorage.getItem('global_sid');
		mui.ajax(url, {
			data: {
				user_id:localStorage.getItem("global_id")
			},
			dataType: "json",
			type: "post",
			timeout: 10000,
			success: function(res) {
				if (res.success) {
					var html1 = template('tpl_group_purchase1', res);
					var html2 = template('tpl_group_purchase2', res);

					if (!res.data.item_list.length) {
                       $('#note_pintuan').css('display','block');
						$('#group_purchase1').html('');
						$('#group_purchase2').html('');
                   }else{
                       	$('#note_pintuan').css('display','none');
						$('#group_purchase1').html(html1);
