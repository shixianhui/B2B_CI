	var stars='';
	
	function get_message_count() {
		if (!is_network(false)) {return;}
		var url = base_url + 'napi/get_message_count?sid=' + localStorage.getItem('global_sid');
		mui.ajax(url, {
			data: {},
			dataType: "json",
			type: "get",
			timeout: 10000,
			success: function(res) {
					console.log(JSON.stringify(res))
				if (res.success) {
					if(res.data.message_count > 0){
						$('#nav_tab i').html(res.data.message_count);
						$('#nav_tab i').css('display','block');
					}else{
						
						$('#nav_tab i').css('display','none');
					}
				} else {
					mui.toast(res.message);
				}
				stars=setTimeout(function(){
					get_message_count();
				},60000)
			},
			error: error
		});
	}
	
	
