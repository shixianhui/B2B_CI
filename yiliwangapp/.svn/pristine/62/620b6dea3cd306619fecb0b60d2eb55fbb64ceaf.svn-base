		mui.ajax(url, {
			data: {},
			dataType: "json",
			type: "get",
			timeout: 10000,
			success: function(res) {
				if (res.success) {
					var html = template('tpl_slider', res);
					$('#slider').html(html);

					// 开启轮播
					var gallery = mui('.mui-slider');
					gallery.slider({
