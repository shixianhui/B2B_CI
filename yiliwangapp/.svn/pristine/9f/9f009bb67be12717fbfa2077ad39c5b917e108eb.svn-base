}

.ad_content_b {
	width: 49%;
}

.ad_content_b .ad_pic {
	width: 100%;
	height: 170px;
	display: block;
	background-size: cover;
	position:relative;
	overflow:hidden;
}
.ad_content_b .ad_pic img{
	height:100%;
	position:absolute;
	left:50%;
	transform:translateX(-50%);
	-webkit-transform:translateX(-50%);
	-moz-transform:translateX(-50%);
}


			
.ad_content_s {
	width: 51%;
	height: 170px;
	overflow: hidden;
	background:#fff;
}

.ad_content_s .ad_pic {
	width: 100%;
	border-bottom: 1px dotted #ccc;
	height: 85px;
	display: block;
	background-size: cover;
	position:relative;
}
.ad_content_s .ad_pic:last-of-type{border-bottom:none;}

.ad_content_s .ad_pic .img{float:right;overflow:hidden;height:74px;width:35%;margin:5px 0;position:relative;}
.ad_content_s .ad_pic .img img{
	position:absolute;
	height:100%;
	left:50%;
	transform:translateX(-50%);
	-webkit-transform:translateX(-50%);
	-moz-transform:translateX(-50%);
}
.ad_content_s .ad_pic .title{float:left; width:65%;background:#fff;height:100%;padding:8px 0px 8px 10px;}
.ad_content_s .ad_pic .title p{color:#333;margin:0;height:3em;}
.ad_content_s .ad_pic .title p.price{color:#b00;margin:0;height:auto;}
.ad_img {
