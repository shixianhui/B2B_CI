.type_list .mui-media-body {
	font-size: 13px;
	color: #323232;
	padding-top: 10px;
	border:none;
}

.headlines {
	height: 45px;
	line-height: 45px;
	position: relative;
	border-top: 1px solid #cbcbcb;
	overflow: hidden;
}

.headlines>strong {
	width: 22%;
	color: #323232;
	position: absolute;
	left: 12px;
}

.headlines>strong span {
	font-size: 13px;
	color: #fff;
	border-radius: 4px;
	background: #e83745;
	padding: 2px 3px;
	margin: 0 2px;
}

.headlines .swiper-wrapper {
	width: 63%;
	height: 45px;
	position: absolute;
}

.headlines .swiper-wrapper li {
	line-height: 45px;
	font-size: 14px;
	white-space: nowrap;
	width: 100%;
	text-overflow: ellipsis;
	overflow: hidden;
}

.headlines .swiper-wrapper a {
	color: #323232;
}

.headlines>.more {
	color: #323232;
	position: absolute;
	right: 12px;
}

.headlines>.more::before {
	content: "";
	width: 1px;
	height: 45%;
	background: #cbcbcb;
	position: absolute;
	top: 12px;
	left: -10px;
}

.day_live {
	background: #fff;
	margin-top: 10px;
	border-bottom: 1px solid #cccccc;
}

.live_title {
	line-height: 36px;
	height: 36px;
	border-bottom: 1px solid #cbcbcb;
}

.live_title h4 {
	line-height: 36px;
	font-weight: bold;
	font-size: 15px;
	color: #323232;
	position: relative;
	left: 50%;
	margin-left: -40px;
}

.live_title h4 span {
	padding: 0 10px;
	background: #fff;
	position: relative;
	z-index: 2;
}

.live_title h4:after {
	width: 150px;
	height: 1px;
	background: #d9d9d9;
	position: absolute;
	content: "";
	top: 17px;
	left: -35px;
}

.live_title .more,
.ad_title .more {
	margin-right: 12px;
	font-size: 13px;
	color: #8a8a8a;
}

.live_title .more .icon-jiantouyou,
.ad_title .more .icon-jiantouyou {
	color: #c81624;
	position: relative;
	top: 1px;
}

.live_list {
	margin: 6px 12px;
	overflow: hidden;
}

.live_list ul {
	width: 100%;
	overflow: hidden;
}

.list_item {
	width: 30%;
