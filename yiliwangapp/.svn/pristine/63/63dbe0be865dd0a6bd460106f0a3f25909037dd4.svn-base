<script type="text/javascript">

    mui('#header').on('tap', 'a', function() {
        var id = this.getAttribute('href');
        if (id == 'm-wdxx.html') {
            go_to_view(id, {
                item_id : "ok"
            });
        }
    });

	mui('.mui-content').on('tap', 'a', function() {
