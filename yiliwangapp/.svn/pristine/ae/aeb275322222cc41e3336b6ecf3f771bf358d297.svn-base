<script src="js/yim.js" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript">
	mui('.live-start-bg').on('tap', 'a', function() {
		var live_id = '111';
		if (plus.os.ios) {
			plus.yim.openLive(live_id, function(){}, function(){});
		} else {
			plus.yim.openLive(live_id);
		}
	});

	mui('#header').on('tap', 'i', function() {
		var type = this.getAttribute('i_type');
		if (type == "close") {
			mui.back();
		} else if (type == 'position') {
			get_site_list();
		}
	});

	window.onload = function() {
		get_site_list();
	}

