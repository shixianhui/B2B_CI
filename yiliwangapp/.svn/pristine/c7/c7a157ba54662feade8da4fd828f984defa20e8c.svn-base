.groups_view .discuss_list .comment_box li .list a{right:8px;top:8px;}
.groups_view .discuss_list .comment_box li .list .sub{font-size:11px;margin-top:5px;}
.groups_view .discuss_list .comment_box li .list .sub font{color:#666;font-size:11px;}
