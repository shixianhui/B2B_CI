.groups_view .discuss_list .comment_box li .reply .sub{font-size:12px;color:#4c4c4;font-weight:normal;}
.groups_view .discuss_list .comment_box li .list{background:#f5f5f5;margin:12px 0;border-radius:8px;padding:8px;position:relative;}
